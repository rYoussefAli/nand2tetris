CHIP And16bit1 {

    IN a[16], b;
    OUT out[16];

    PARTS:

    And4 (a=a[0..3], b[0]=b, b[1]=b, b[2]=b, b[3]=b, out=out[0..3]);
    And4 (a=a[4..7], b[0]=b, b[1]=b, b[2]=b, b[3]=b, out=out[4..7]);
    And4 (a=a[8..11], b[0]=b, b[1]=b, b[2]=b, b[3]=b, out=out[8..11]);
    And4 (a=a[12..15], b[0]=b, b[1]=b, b[2]=b, b[3]=b, out=out[12..15]);
}